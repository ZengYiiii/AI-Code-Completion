# VS Code extension files
.vscode/**
.vscode-test/**

# Source files (we only need compiled output)
src/**
**/*.ts
**/*.map

# Configuration files
**/tsconfig.json
**/.eslintrc.json
**/tslint.json

# Dependencies will be included, but we can exclude some
node_modules/**/.bin/**
node_modules/**/test/**
node_modules/**/tests/**
node_modules/**/node_modules/**
node_modules/**/coverage/**
node_modules/**/docs/**
node_modules/**/examples/**
node_modules/**/*.md
node_modules/**/*.txt
node_modules/**/CHANGELOG*
node_modules/**/LICENSE*
node_modules/**/README*

# Git and build artifacts
.gitignore
.yarnrc
**/yarn.lock
**/package-lock.json
**/npm-debug.log*
**/yarn-debug.log*
**/yarn-error.log*

# Development files
vsc-extension-quickstart.md
*.tgz
*.tar.gz

# OS specific
.DS_Store
Thumbs.db

# IDE specific
.vscode/settings.json
.vscode/launch.json
.vscode/extensions.json
.idea/
*.swp
*.swo

# Logs
logs
*.log

# Runtime data
pids
*.pid
*.seed
*.pid.lock

# Coverage directory used by tools like istanbul
coverage/

# Temporary folders
tmp/
temp/

